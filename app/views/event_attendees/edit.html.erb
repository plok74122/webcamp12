<h1>Edit Attendee (<%= @event.name %>)</h1>

<%= form_for @attendee, :url => event_attendee_path(@event,@attendee), :method => :patch do |f| %>
    <div class="form-group">
      <%= f.label :username %>
      <%= f.text_field :username ,:class => "form-control"%>
    </div>
    <div class="form-group">
      <%= f.submit :class => "btn btn-danger" %>
    </div>
<% end %>